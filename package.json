{
  "name": "bash-eval",
  "version": "1.0.0",
  "description": "Eval comparing bash/filesystem vs SQL vs embeddings for AI agent data exploration",
  "type": "module",
  "scripts": {
    "cli": "tsx src/cli.tsx",
    "debug": "tsx src/debug-agent.ts",
    "download": "tsx src/data/download.ts",
    "transform": "tsx src/data/transform.ts",
    "transform:codemode": "tsx src/data/codemode-transform.ts",
    "embed": "tsx src/data/embed.ts",
    "eval": "npx braintrust eval evals/sql.eval.ts evals/bash.eval.ts evals/fs.eval.ts evals/embedding.eval.ts evals/codemode.eval.ts",
    "eval:sql": "npx braintrust eval evals/sql.eval.ts",
    "eval:bash": "npx braintrust eval evals/bash.eval.ts",
    "eval:fs": "npx braintrust eval evals/fs.eval.ts",
    "eval:embedding": "npx braintrust eval evals/embedding.eval.ts",
    "eval:codemode": "npx braintrust eval evals/codemode.eval.ts",
    "validate": "tsx src/validate-questions.ts",
    "prepare": "husky",
    "lint": "oxlint --deny-warnings",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx}'"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "oxlint --deny-warnings"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.9",
    "@ai-sdk/openai": "^3.0.7",
    "@ai-sdk/provider-utils": "4.0.4",
    "@inquirer/prompts": "^7.4.0",
    "ai": "^6.0.27",
    "autoevals": "^0.0.131",
    "bash-tool": "^1.3.1",
    "better-sqlite3": "^11.8.1",
    "braintrust": "^2.0.1",
    "chalk": "^5.4.1",
    "dotenv": "^17.2.3",
    "glob": "^11.0.1",
    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "just-bash": "^2.4.0",
    "openai": "^4.103.0",
    "react": "^19.2.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.10.7",
    "@types/react": "^19.2.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxlint": "^1.38.0",
    "prettier": "^3.7.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
